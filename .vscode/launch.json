  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File with Arguments",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      //   "args": "${command:pickArgs}"
      "justMyCode": true,
      "args": [
        "--pdf_file",
        "CIS_CentOS_Linux_7_Benchmark_v4.0.0.pdf",
        "--out_file",
        "CIS_CentOS_Linux_7_Benchmark_v4.0.0.csv"
      ]
    }
  ]
}
